{
  "name": "chess-backend",
  "version": "1.0.0",
  "description": "Chess.com clone backend server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --verbose",
    "seed:puzzles": "ts-node src/scripts/seedPuzzles.ts",
    "bootstrap:admin": "node src/scripts/bootstrap-admin.js",
    "seed:bots": "node src/scripts/seed-bots.js"
  },
  "keywords": [
    "chess",
    "game",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chess.js": "^1.0.0-beta.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "socket.io": "^4.6.1",
    "stockfish.js": "^10.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-microsoft": "^1.0.0",
    "@types/passport-jwt": "^3.0.9",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^6.0.3",
    "jest": "^29.6.1",
    "socket.io-client": "^4.8.3",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
